<h1>Liste des tracteurs</h1>

<table>
  <tr>
    <th>Modele</th>
    <th>Year</th>
    <th>Bonus</th>
    <th>Cost</th>
    <th>Serial</th>
    <th>Stock</th>
    <th>Arrival</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tracteurs.each do |tracteur| %>
  <tr>
    <td><%= tracteur.modele %></td>
    <td><%= tracteur.annee %></td>
    <td><%= tracteur.bonus %></td>
    <td><%= tracteur.cost %></td>
    <td><%= tracteur.serie %></td>
    <td><%= tracteur.stock %></td>
    <td><%= tracteur.arrivee %></td>
    <td><%= link_to 'Show', tracteur %></td>
    <td><%= link_to 'Edit', edit_tracteur_path(tracteur) %></td>
    <td><%= link_to 'Destroy', tracteur, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />
<%= link_to 'New Tracteur', new_tracteur_path %>
